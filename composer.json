{
  "name": "byjg/convert",
  "description": "A lightweight utility for string conversion between text from UTF8 to a lot of formats and vice-versa. ",
  "autoload": {
    "psr-4": {
      "ByJG\\Convert\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1 <8.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6|^11",
    "vimeo/psalm": "^5.9|^6.13"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm --threads=1"
  },
  "license": "MIT"
}
